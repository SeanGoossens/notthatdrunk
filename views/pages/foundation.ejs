<!doctype html>
<html class="no-js" lang="en">
  <style>
    table, th, td {
      border:1px solid black;
    }
    </style>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Not That Drunk</title>
    <link rel="stylesheet" href="css/app.css">
  </head>
  <body>
    <div class="grid-container">
      <div class="grid-x grid-padding-x">
        <div class="large-12 cell">
          <h1>Top Not That Drunk M+ Scores</h1>
        </div>
      </div>
      
      <div class="grid-x grid-padding-x">
        <div class="large-12 cell">
          <div class="callout">
            <table style="width:50%">
                <tr>
                  <td>Rank</td>
                  <td>Player</td>
                  <td>M+ Score</td>
                  <td>Role</td>
                  <td>Overall Rank</td>
                  <td>Role Rank</td>
                </tr>
                <tr>
                <% for(var i=0; i < playerArray.length; i++) { %>
                  <tr>
                    <td><%= playerArray[i].id %></td>
                    <td><%= playerArray[i].playerName %></td>
                    <td><%= playerArray[i].score %></td>
                    <td><%= playerArray[i].role %></td>
                    <td><%= playerArray[i].overallRank %></td>
                    <td><%= playerArray[i].roleRank %></td>
                  </tr>
               <% } %>
               </table>
            
    

      

    <script src="node_modules/jquery/dist/jquery.js"></script>
    <script src="node_modules/what-input/dist/what-input.js"></script>
    <script src="node_modules/foundation-sites/dist/js/foundation.js"></script>
    <script src="js/app.js"></script>
  </body>
</html>
